{
  "manifest_version": 2,

  "name": "Docbot",
  "short_name": "Curated Search Engine for Teams",
  "description": "Docbot plugin for integration with Docbot Slack integration.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html",
    "default_title": "Docbot"
  },

  "background": {
    "scripts": ["index.js"],
    "persistent": false
  },

  "permissions": [
    "activeTab",
    "https://temp-chrome-ext-server-xzmjibnxyh.now.sh/chrome",
    "https://ajax.googleapis.com/"
  ]
}
